<!-- network config -->
<yandex>
    <http_port>8123</http_port>
    <tcp_port>9000</tcp_port>
    
    <listen_host>0.0.0.0</listen_host>
    
    <http_server_default_response>
        <name>clickhouse</name>
    </http_server_default_response>

    <!-- allow all ip -->
    <networks>
        <ip>::/0</ip>
    </networks>
</yandex>